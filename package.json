{
  "name": "myant",
  "version": "0.1.0",
  "description": "WX & AliPay UI Components built on uniApp",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "scripts": {
    "lint": "npx myant-cli lint",
    "test": "npx myant-cli test",
    "dev:mini": "npx myant-cli dev:mini --target mini",
    "dev:site": "npx myant-cli dev:mini --target site",
    "build": "npx myant-cli build",
    "release": "npx myant-cli release"
  },
  "repository": "https://github.com/miya-fe/myant",
  "sideEffects": false,
  "files": [
    "es",
    "lib",
    "types"
  ],
  "keywords": [
    "myant",
    "miya-ant",
    "ant",
    "uniapp",
    "wx",
    "weixin",
    "alipay",
    "components"
  ],
  "author": "miya-fe",
  "license": "MIT",
  "devDependencies": {
    "@myant/cli": "^1.0.4",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@myant/vue-ts"
    ]
  },
  "stylelint": {
    "extends": [
      "@myant/stylelint-config"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "endOfLine": "lf",
    "proseWrap": "never"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ]
}
